{
  "name": "vgc-multicalc",
  "version": "1.7.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage --watch=false",
    "test-watch": "ng test --code-coverage",
    "e2e-test": "npx cypress run --quiet; exit_code=$?; if [ $exit_code -eq 0 ]; then echo '✅ Tests PASSED successfully!\n'; else echo '❌ Tests FAILED.\n'; fi; exit $exit_code",
    "lint": "ng lint",
    "formatter": "npx prettier . --check",
    "check": "npm run lint && npm run formatter"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.3.2",
    "@angular/cdk": "20.2.5",
    "@angular/common": "20.3.2",
    "@angular/compiler": "20.3.2",
    "@angular/core": "20.3.2",
    "@angular/forms": "20.3.2",
    "@angular/material": "20.2.5",
    "@angular/platform-browser": "20.3.2",
    "@angular/platform-browser-dynamic": "20.3.2",
    "@angular/router": "20.3.2",
    "@ngrx/signals": "20.0.1",
    "@robsonbittencourt/calc": "0.10.3",
    "axios": "1.12.2",
    "dedent": "1.7.0",
    "koffing": "1.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.3",
    "@angular/cli": "20.3.3",
    "@angular/compiler-cli": "20.3.2",
    "@types/jasmine": "5.1.9",
    "@types/uuid": "11.0.0",
    "angular-cli-ghpages": "2.0.3",
    "angular-eslint": "20.3.0",
    "cypress": "15.3.0",
    "cypress-real-events": "1.15.0",
    "eslint": "9.36.0",
    "jasmine-core": "5.11.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0"
  },
  "overrides": {
    "@ngrx/signals": {
      "@angular/common": "$@angular/common",
      "@angular/core": "$@angular/core"
    }
  }
}
