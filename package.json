{
  "name": "vgc-multicalc",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage --watch=false",
    "test-watch": "ng test --code-coverage",
    "e2e-test": "npx cypress run --quiet; exit_code=$?; if [ $exit_code -eq 0 ]; then echo '✅ Tests PASSED successfully!\n'; else echo '❌ Tests FAILED.\n'; fi; exit $exit_code",
    "lint": "ng lint",
    "formatter": "npx prettier . --check",
    "check": "npm run lint && npm run formatter"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.1.4",
    "@angular/cdk": "19.1.2",
    "@angular/common": "19.1.4",
    "@angular/compiler": "19.1.4",
    "@angular/core": "19.1.4",
    "@angular/forms": "19.1.4",
    "@angular/material": "19.1.2",
    "@angular/platform-browser": "19.1.4",
    "@angular/platform-browser-dynamic": "19.1.4",
    "@angular/router": "19.1.4",
    "@ngrx/signals": "19.0.1",
    "@robsonbittencourt/calc": "0.10.1",
    "axios": "1.7.9",
    "dedent": "1.5.3",
    "koffing": "1.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.8.1",
    "uuid": "11.0.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.1.5",
    "@angular/cli": "19.1.5",
    "@angular/compiler-cli": "19.1.4",
