{
  "name": "vgc-multicalc",
  "version": "1.8.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage --watch=false",
    "test-watch": "ng test --code-coverage",
    "e2e-test": "npx cypress run --quiet; exit_code=$?; if [ $exit_code -eq 0 ]; then echo '✅ Tests PASSED successfully!\n'; else echo '❌ Tests FAILED.\n'; fi; exit $exit_code",
    "lint": "ng lint",
    "formatter": "npx prettier . --check",
    "check": "npm run lint && npm run formatter"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.3.9",
    "@angular/cdk": "20.2.11",
    "@angular/common": "20.3.9",
    "@angular/compiler": "20.3.9",
    "@angular/core": "20.3.9",
    "@angular/forms": "20.3.9",
    "@angular/material": "20.2.11",
    "@angular/platform-browser": "20.3.9",
    "@angular/platform-browser-dynamic": "20.3.9",
    "@angular/router": "20.3.9",
    "@ngrx/signals": "20.1.0",
    "@robsonbittencourt/calc": "0.10.4",
    "axios": "1.13.1",
    "dedent": "1.7.0",
    "koffing": "1.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.8",
    "@angular/cli": "20.3.8",
    "@angular/compiler-cli": "20.3.9",
    "@types/jasmine": "5.1.12",
    "@types/uuid": "11.0.0",
    "angular-cli-ghpages": "2.0.3",
    "angular-eslint": "20.5.1",
    "cypress": "15.5.0",
    "cypress-real-events": "1.15.0",
    "eslint": "9.39.0",
    "jasmine-core": "5.12.1",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2"
  },
  "overrides": {
    "@ngrx/signals": {
      "@angular/common": "$@angular/common",
      "@angular/core": "$@angular/core"
    }
  }
}
