    "@types/uuid": "10.0.0",
    "angular-cli-ghpages": "2.0.3",
    "angular-eslint": "19.0.2",
    "cypress": "14.0.1",
    "eslint": "^9.19.0",
    "jasmine-core": "5.5.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.4.2",
    "typescript": "5.6.3",
    "typescript-eslint": "8.23.0"
  }
