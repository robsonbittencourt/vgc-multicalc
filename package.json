{
  "name": "vgc-multicalc",
  "version": "1.6.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage --watch=false",
    "test-watch": "ng test --code-coverage",
    "e2e-test": "npx cypress run --quiet; exit_code=$?; if [ $exit_code -eq 0 ]; then echo '✅ Tests PASSED successfully!\n'; else echo '❌ Tests FAILED.\n'; fi; exit $exit_code",
    "lint": "ng lint",
    "formatter": "npx prettier . --check",
    "check": "npm run lint && npm run formatter"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.0.6",
    "@angular/cdk": "20.0.5",
    "@angular/common": "20.0.6",
    "@angular/compiler": "20.0.6",
    "@angular/core": "20.0.6",
    "@angular/forms": "20.0.6",
    "@angular/material": "20.0.5",
    "@angular/platform-browser": "20.0.6",
    "@angular/platform-browser-dynamic": "20.0.6",
    "@angular/router": "20.0.6",
    "@ngrx/signals": "19.2.1",
    "@robsonbittencourt/calc": "0.10.2",
    "axios": "1.10.0",
    "dedent": "1.6.0",
    "koffing": "1.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.5",
    "@angular/cli": "20.0.5",
    "@angular/compiler-cli": "20.0.6",
    "@types/jasmine": "5.1.8",
    "@types/uuid": "10.0.0",
    "angular-cli-ghpages": "2.0.3",
    "angular-eslint": "20.1.1",
    "cypress": "14.5.1",
    "cypress-real-events": "1.14.0",
    "eslint": "9.30.1",
    "jasmine-core": "5.8.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.36.0"
  },
  "overrides": {
    "@ngrx/signals": {
      "@angular/common": "$@angular/common",
      "@angular/core": "$@angular/core"
    }
  }
}
